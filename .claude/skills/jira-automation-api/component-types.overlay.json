      "target": "$.components.schemas",
      "description": "Add reusable schemas and per-type value schemas",
      "update": {
        "FieldReference": {
          "type": "object",
          "description": "Reusable field reference pattern used throughout component values.",
          "properties": {
            "type": {
              "type": "string",
              "enum": ["ID", "NAME", "COPY", "SMART", "FREE", "REFERENCE"],
              "description": "ID=by field ID, NAME=by display name, COPY=from issue (current/trigger/created), SMART=smart value expression {{...}}, FREE=literal text, REFERENCE=known entity (LAST_COMMENT)"
            },
            "value": { "type": "string" }
          },
          "required": ["type", "value"]
        },
        "OperationsArray": {
          "type": "array",
          "description": "Used by edit/create/clone/transition actions to set issue fields.",
          "items": {
            "type": "object",
            "properties": {
              "field": { "$ref": "#/components/schemas/FieldReference" },
              "fieldType": { "type": "string" },
              "type": { "type": "string", "enum": ["SET", "APPEND", "REMOVE"] },
              "value": {}
            },
            "required": ["field", "fieldType", "type", "value"]
          }
        },

